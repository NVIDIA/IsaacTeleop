PLACEHOLDER URDF - Replace with your robot hand URDF

This is a minimal example URDF for a 6-DOF dexterous hand.
For actual use, you need to provide your robot-specific URDF file.

Example sources for real URDFs:
- IsaacLab Nucleus: https://docs.omniverse.nvidia.com/isaacsim/latest/isaac_lab_tutorials/tutorial_assets.html
- Your robot manufacturer's documentation
- Generate from CAD using tools like phobos/urdf-exporter

Required structure:
- Wrist link (base_link or hand_base_link)
- 5 fingers: thumb, index, middle, ring, pinky
- Each finger has: tip link (for retargeting)
- Joint names matching the YAML config
-->
<robot name="example_hand">
  <!-- Base wrist link -->
  <link name="L_hand_base_link">
    <inertial>
      <mass value="0.1"/>
      <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/>
    </inertial>
  </link>

  <!-- Thumb joints -->
  <joint name="L_thumb_proximal_yaw_joint" type="revolute">
    <parent link="L_hand_base_link"/>
    <child link="L_thumb_proximal"/>
    <axis xyz="0 0 1"/>
    <limit lower="-1.57" upper="1.57" effort="10" velocity="1.0"/>
  </joint>
  <link name="L_thumb_proximal">
    <inertial>
      <mass value="0.01"/>
      <inertia ixx="0.0001" ixy="0" ixz="0" iyy="0.0001" iyz="0" izz="0.0001"/>
    </inertial>
  </link>

  <joint name="L_thumb_proximal_pitch_joint" type="revolute">
    <parent link="L_thumb_proximal"/>
    <child link="L_thumb_tip"/>
    <axis xyz="0 1 0"/>
    <limit lower="0" upper="1.57" effort="10" velocity="1.0"/>
  </joint>
  <link name="L_thumb_tip">
    <inertial>
      <mass value="0.01"/>
      <inertia ixx="0.0001" ixy="0" ixz="0" iyy="0.0001" iyz="0" izz="0.0001"/>
    </inertial>
  </link>

  <!-- Index finger -->
  <joint name="L_index_proximal_joint" type="revolute">
    <parent link="L_hand_base_link"/>
    <child link="L_index_proximal"/>
    <axis xyz="0 1 0"/>
    <limit lower="0" upper="1.57" effort="10" velocity="1.0"/>
  </joint>
  <link name="L_index_proximal">
    <inertial>
      <mass value="0.01"/>
      <inertia ixx="0.0001" ixy="0" ixz="0" iyy="0.0001" iyz="0" izz="0.0001"/>
    </inertial>
  </link>
  <joint name="L_index_distal_joint" type="revolute">
    <parent link="L_index_proximal"/>
    <child link="L_index_tip"/>
    <axis xyz="0 1 0"/>
    <limit lower="0" upper="1.57" effort="10" velocity="1.0"/>
  </joint>
  <link name="L_index_tip">
    <inertial>
      <mass value="0.01"/>
      <inertia ixx="0.0001" ixy="0" ixz="0" iyy="0.0001" iyz="0" izz="0.0001"/>
    </inertial>
  </link>

  <!-- Middle finger -->
  <joint name="L_middle_proximal_joint" type="revolute">
    <parent link="L_hand_base_link"/>
    <child link="L_middle_proximal"/>
    <axis xyz="0 1 0"/>
    <limit lower="0" upper="1.57" effort="10" velocity="1.0"/>
  </joint>
  <link name="L_middle_proximal">
    <inertial>
      <mass value="0.01"/>
      <inertia ixx="0.0001" ixy="0" ixz="0" iyy="0.0001" iyz="0" izz="0.0001"/>
    </inertial>
  </link>
  <joint name="L_middle_distal_joint" type="revolute">
    <parent link="L_middle_proximal"/>
    <child link="L_middle_tip"/>
    <axis xyz="0 1 0"/>
    <limit lower="0" upper="1.57" effort="10" velocity="1.0"/>
  </joint>
  <link name="L_middle_tip">
    <inertial>
      <mass value="0.01"/>
      <inertia ixx="0.0001" ixy="0" ixz="0" iyy="0.0001" iyz="0" izz="0.0001"/>
    </inertial>
  </link>

  <!-- Ring finger -->
  <joint name="L_ring_proximal_joint" type="revolute">
    <parent link="L_hand_base_link"/>
    <child link="L_ring_proximal"/>
    <axis xyz="0 1 0"/>
    <limit lower="0" upper="1.57" effort="10" velocity="1.0"/>
  </joint>
  <link name="L_ring_proximal">
    <inertial>
      <mass value="0.01"/>
      <inertia ixx="0.0001" ixy="0" ixz="0" iyy="0.0001" iyz="0" izz="0.0001"/>
    </inertial>
  </link>
  <joint name="L_ring_distal_joint" type="revolute">
    <parent link="L_ring_proximal"/>
    <child link="L_ring_tip"/>
    <axis xyz="0 1 0"/>
    <limit lower="0" upper="1.57" effort="10" velocity="1.0"/>
  </joint>
  <link name="L_ring_tip">
    <inertial>
      <mass value="0.01"/>
      <inertia ixx="0.0001" ixy="0" ixz="0" iyy="0.0001" iyz="0" izz="0.0001"/>
    </inertial>
  </link>

  <!-- Pinky finger -->
  <joint name="L_pinky_proximal_joint" type="revolute">
    <parent link="L_hand_base_link"/>
    <child link="L_pinky_proximal"/>
    <axis xyz="0 1 0"/>
    <limit lower="0" upper="1.57" effort="10" velocity="1.0"/>
  </joint>
  <link name="L_pinky_proximal">
    <inertial>
      <mass value="0.01"/>
      <inertia ixx="0.0001" ixy="0" ixz="0" iyy="0.0001" iyz="0" izz="0.0001"/>
    </inertial>
  </link>
  <joint name="L_pinky_distal_joint" type="revolute">
    <parent link="L_pinky_proximal"/>
    <child link="L_pinky_tip"/>
    <axis xyz="0 1 0"/>
    <limit lower="0" upper="1.57" effort="10" velocity="1.0"/>
  </joint>
  <link name="L_pinky_tip">
    <inertial>
      <mass value="0.01"/>
      <inertia ixx="0.0001" ixy="0" ixz="0" iyy="0.0001" iyz="0" izz="0.0001"/>
    </inertial>
  </link>

</robot>
